---
import WorldExplorer from '../components/WorldExplorer.jsx';
import '../styles/globals.css';
const { title, path } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
  </head>
  <body>
    <WorldExplorer path={path} client:load />
  </body>
</html>
<parameter name="filePath"></parameter>
